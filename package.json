    "react-native-navigation": "^2.21.0",
    "react-native-vector-icons": "^6.5.0",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
